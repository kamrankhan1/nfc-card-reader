<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Support for NDEF formatted tags -->
    <tech-list>
        <tech>android.nfc.tech.Ndef</tech>
    </tech-list>
    
    <!-- Support for NDEF formattable tags -->
    <tech-list>
        <tech>android.nfc.tech.NdefFormatable</tech>
    </tech-list>
    
    <!-- Support for NFC-A (ISO 14443-3A) tags -->
    <tech-list>
        <tech>android.nfc.tech.NfcA</tech>
    </tech-list>
    
    <!-- Support for ISO-DEP (ISO 14443-4) tags -->
    <tech-list>
        <tech>android.nfc.tech.IsoDep</tech>
    </tech-list>
    
    <!-- Support for MIFARE Classic tags -->
    <tech-list>
        <tech>android.nfc.tech.MifareClassic</tech>
    </tech-list>
    
    <!-- Support for MIFARE Ultralight tags -->
    <tech-list>
        <tech>android.nfc.tech.MifareUltralight</tech>
    </tech-list>
</resources>
